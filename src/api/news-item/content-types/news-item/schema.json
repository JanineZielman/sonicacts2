{
  "kind": "collectionType",
  "collectionName": "news_items",
  "info": {
    "singularName": "news-item",
    "pluralName": "news-items",
    "displayName": "News Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid"
    },
    "title": {
      "type": "string"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "basic.image",
        "basic.text"
      ]
    },
    "community_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::community-item.community-item"
    },
    "cover_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
