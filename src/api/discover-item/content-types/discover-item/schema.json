{
  "kind": "collectionType",
  "collectionName": "discover_items",
  "info": {
    "singularName": "discover-item",
    "pluralName": "discover-items",
    "displayName": "Discover Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "basic.image",
        "basic.text",
        "basic.sidenote"
      ]
    },
    "title": {
      "type": "string"
    },
    "community_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::community-item.community-item"
    },
    "links": {
      "type": "richtext"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "discover_items"
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "tag": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tag.tag",
      "inversedBy": "discover_items"
    }
  }
}
